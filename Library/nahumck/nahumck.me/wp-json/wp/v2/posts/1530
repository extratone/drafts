\/post"}],"author":[{"embeddable":true,"href":"https:\/\/nahumck.me\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"https:\/\/nahumck.me\/wp-json\/wp\/v2\/comments?post=1530"}],"version-history":[{"count":8,"href":"https:\/\/nahumck.me\/wp-json\/wp\/v2\/posts\/1530\/revisions"}],"predecessor-version":[{"id":1687,"href":"https:\/\/nahumck.me\/wp-json\/wp\/v2\/posts\/1530\/revisions\/1687"}],"wp:attachment":[{"href":"https:\/\/nahumck.me\/wp-json\/wp\/v2\/media?parent=1530"}],"wp:term":[{"taxonomy":"category","embeddable":true,"href":"https:\/\/nahumck.me\/wp-json\/wp\/v2\/categories?post=1530"},{"taxonomy":"post_tag","embeddable":true,"href":"https:\/\/nahumck.me\/wp-json\/wp\/v2\/tags?post=1530"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}